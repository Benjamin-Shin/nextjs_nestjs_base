{
  "extends": ["next/core-web-vitals", "prettier"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "ignorePatterns": ["build", "dist", "public"],
  "rules": {
    "no-console": "error",
    "import/prefer-default-export": "off",
    "prettier/prettier": ["error"]
  },
  "plugins": ["prettier"]
}
